@{
}
@model IEnumerable<string>
<div class="btn btn-block">
    <a asp-controller="Home" asp-action="Index" asp-route-projectType="" asp-route-pageNum="" class="btn btn-outline-dark">Home</a>

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t"  asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == t ?  "btn-dark" : "btn-outline-secondary")">@t</a>
    }
</div>